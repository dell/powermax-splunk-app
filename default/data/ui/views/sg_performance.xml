                <query>`powermax_sg`
                    array_id=$array_tok$
                    storage_group_id=$sg_tok$
                    service_level=$sl_tok$
                    workload=$wl_tok$
                    device_emulation=$emu_tok$
                    type=$type_tok$
                    compression=$compression_tok$
                    slo_compliance=$compliance_tok$
                    | dedup srp
                    | table srp
                    | sort srp
                </query>
                <earliest>$time_tok.earliest$</earliest>
                <latest>$time_tok.latest$</latest>
